AA,WAAA,CAAA,IAAA,GACA,8BADA,CACA,KAAA,KADA,EACA,KAAA,QADA,EAEA,IAFA,CAEA,YAAA;AACA,QAAA,WAAA,CAAA,IAAA,GACA,WADA,CACA,aADA,CACA;AACA,UAAA,WAAA,EAAA,KAAA,CAAA;AADA,SADA,EAIA,IAJA,CAIA,YAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,WAAA,CAAA,IAAA,GAAA,WAAA;AACA,UAAA,EAAA,CAAA,UAAA,CAAA,OAAA,EACA,GADA,CACA,WAAA,CAAA,IAAA,GAAA,WAAA,CAAA,GADA,EAEA,GAFA,CAEA;AACA,YAAA,SAAA,EAAA,KAAA,CAAA,SADA;AAEA,YAAA,QAAA,EAAA,KAAA,CAAA,QAFA;AAGA,YAAA,QAAA,EAAA,KAAA,CAAA,SAHA;AAIA,YAAA,KAAA,EAAA,KAAA,CAAA;AAJA,WAFA,EAQA,IARA,CAQA,YAAA;AACA,YAAA,MAAA,CAAA,EAAA,CAAA;AAAA,cAAA,IAAA,EAAA;AAAA,aAAA;AACA,WAVA,EAWA,KAXA,CAWA,UAAA,GAAA,EAAA;AACA,YAAA,KAAA,CAAA,YAAA,GAAA,GAAA,CAAA,OAAA;AACA,WAbA;AAcA,SApBA,EAqBA,KArBA,CAqBA,UAAA,GAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,UAAA,KAAA,CAAA,YAAA,GAAA,GAAA,CAAA,OAAA;AACA,SAxBA;AAyBA,OA5BA,EA6BA,KA7BA,CA6BA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,QAAA,KAAA,CAAA,YAAA,GAAA,GAAA,CAAA,OAAA;AACA,OAhCA;AAiCA,MAAA,CAAA,CAAA,cAAA;AACA;AApCA;AAVA,CAAA","sourcesContent":["<template>\r\n  <div id=\"signUp\" class=\"container\">\r\n    <div class=\"row\">\r\n      <div class=\"col-sm-8 col-md-8 col-lg-8 mx-auto\">\r\n        <div class=\"card card-signin my-5\">\r\n          <div class=\"card-body\">\r\n            <h5 class=\"card-title\">Time to make shopping a whole lot easier!</h5>\r\n\r\n            <form class=\"form-signin\">\r\n              <div class=\"row\">\r\n                <div class=\"col-md-6 form-label-group\">\r\n                  <input\r\n                    type=\"text\"\r\n                    id=\"firstname\"\r\n                    class=\"form-control\"\r\n                    placeholder=\"First Name\"\r\n                    v-model=\"firstname\"\r\n                  />\r\n                </div>\r\n                <br />\r\n                <div class=\"col-md-6 form-label-group\">\r\n                  <input\r\n                    type=\"text\"\r\n                    id=\"lastname\"\r\n                    class=\"form-control\"\r\n                    placeholder=\"Last Name\"\r\n                    v-model=\"lastname\"\r\n                  />\r\n                </div>\r\n              </div>\r\n\r\n              <br />\r\n              <div class=\"form-label-group\">\r\n                <input\r\n                  type=\"email\"\r\n                  id=\"email\"\r\n                  class=\"form-control\"\r\n                  placeholder=\"Email address\"\r\n                  v-model=\"email\"\r\n                />\r\n              </div>\r\n              <br />\r\n              <div class=\"form-label-group\">\r\n                <input\r\n                  type=\"password\"\r\n                  id=\"password\"\r\n                  class=\"form-control\"\r\n                  placeholder=\"Password\"\r\n                  v-model=\"password\"\r\n                />\r\n              </div>\r\n              <div class=\"form-label-group\">\r\n                <input\r\n                  type=\"password\"\r\n                  id=\"password-confirm\"\r\n                  class=\"form-control\"\r\n                  placeholder=\"Confirm Password\"\r\n                />\r\n              </div>\r\n              <button class=\"btn btn-lg btn-outline-primary\" @click=\"signup\" type=\"submit\">Sign Up</button>\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <!--Sign In -->\r\n      <div class=\"col-sm-4 col-md-4 col-lg-4 mx-auto\">\r\n        <div class=\"card card-signin my-5\">\r\n          <div class=\"card-body\">\r\n            <h5 class=\"card-title text-center\">Already Have an Account?</h5>\r\n            <p class=\"text-center\">\r\n              If you already has an account, go ahead and sign in. We've missed\r\n              you!\r\n            </p>\r\n            <router-link class=\"btn btn-lg btn-primary btn-block\" to=\"/signin\">Sign In</router-link>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport firebaseApp from \"../firebaseInit\";\r\nimport router from \"../router\";\r\nconst db = firebaseApp.firestore();\r\n\r\nexport default {\r\n  name: \"signup\",\r\n  data: function() {\r\n    return {\r\n      email: \"\",\r\n      password: \"\",\r\n      firstname: \"\",\r\n      lastname: \"\"\r\n    };\r\n  },\r\n  methods: {\r\n    signup: function(e) {\r\n      firebaseApp.auth()\r\n        .createUserWithEmailAndPassword(this.email, this.password)\r\n        .then(() => {\r\n          firebaseApp.auth()\r\n            .currentUser.updateProfile({\r\n              displayName: this.firstname\r\n            })\r\n            .then(() => {\r\n              console.log(firebaseApp.auth().currentUser);\r\n              db.collection(\"users\")\r\n                .doc(firebaseApp.auth().currentUser.uid)\r\n                .set({\r\n                  firstname: this.firstname,\r\n                  lastname: this.lastname,\r\n                  username: this.firstname,\r\n                  email: this.email\r\n                })\r\n                .then(() => {\r\n                  router.go({ name: \"groceryDashboard\" });\r\n                })\r\n                .catch(err => {\r\n                  this.errorMessage = err.message;\r\n                });\r\n            })\r\n            .catch(err => {\r\n              console.log(err);\r\n              this.errorMessage = err.message;\r\n            });\r\n        })\r\n        .catch(err => {\r\n          console.log(err);\r\n          this.errorMessage = err.message;\r\n        });\r\n      e.preventDefault();\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style scoped>\r\n.btn {\r\n  display: inline-block;\r\n  background: var(--primary-color);\r\n  color: #fff;\r\n  padding: 0.8rem 1.5rem;\r\n  border: none;\r\n  cursor: pointer;\r\n  font-size: 0.9rem;\r\n  border-radius: 30px;\r\n  margin-top: 0.2rem;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}