ersion":3,"sources":["VerifyDetails.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgNA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"VerifyDetails.vue","sourceRoot":"src/components","sourcesContent":["<template>\r\n  <div id=\"verify-details\" class=\"container-fluid bg-light\">\r\n    <div class=\"row\">\r\n      <Sidebar />\r\n      <div class=\"col-md-6 ml-sm-auto col-lg-9 pt-4 pl-4\">\r\n        <div class=\"container\">\r\n          <div\r\n            class=\"d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pb-1 mb-3 border-bottom\"\r\n          >\r\n            <h1 v-if=\"!verified\" class=\"h2\">Store Login</h1>\r\n            <h1 v-if=\"verified\" class=\"h2\">\r\n              Change Your Delivery Address\r\n            </h1>\r\n          </div>\r\n          <div class=\"container\">\r\n            <div class=\"card\">\r\n              <div class=\"card-body\">\r\n                <div class=\"content\">\r\n                  <form\r\n                    v-if=\"!verified\"\r\n                    id=\"signup-form\"\r\n                    @submit.prevent=\"processForm\"\r\n                  >\r\n                    <div class=\"row\">\r\n                      <div class=\"col-md-6\">\r\n                        <div class=\"form-group\">\r\n                          <label>Email</label>\r\n                          <input\r\n                            type=\"text\"\r\n                            name=\"email\"\r\n                            v-model=\"email\"\r\n                            class=\"form-control border-input\"\r\n                            placeholder=\"JohnDowe@mail.com\"\r\n                          />\r\n                        </div>\r\n                      </div>\r\n                      <div class=\"col-md-6\">\r\n                        <div class=\"form-group\">\r\n                          <label>Password</label>\r\n                          <input\r\n                            type=\"password\"\r\n                            name=\"password\"\r\n                            v-model=\"password\"\r\n                            class=\"form-control border-input\"\r\n                            placeholder=\"Password\"\r\n                          />\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n\r\n                    <div class=\"row\">\r\n                      <div class=\"col-md-12\">\r\n                        <div class=\"form-group\">\r\n                          <label>Delivery Address</label>\r\n                          <input\r\n                            type=\"text\"\r\n                            name=\"address\"\r\n                            v-model=\"address\"\r\n                            class=\"form-control border-input\"\r\n                            placeholder=\"Home Address\"\r\n                          />\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n\r\n                    <div class=\"text-center\">\r\n                      <!--<button type=\"submit\" class=\"btn btn-info btn-fill btn-wd p-2\">Update Details</button>-->\r\n                      <button\r\n                        type=\"submit\"\r\n                        class=\"btn btn-success btn-fill btn-wd p-2 m-2\"\r\n                      >\r\n                        Sign In\r\n                      </button>\r\n                    </div>\r\n                    <div class=\"clearfix\"></div>\r\n                  </form>\r\n                  <!--End Of Store Login-->\r\n\r\n                  <form\r\n                    v-if=\"verified\"\r\n                    id=\"address-form\"\r\n                    @submit.prevent=\"updateAddress\"\r\n                  >\r\n                    <div class=\"row\">\r\n                      <div class=\"col-md-12\">\r\n                        <div class=\"form-group\">\r\n                          <label>Delivery Address</label>\r\n                          <input\r\n                            type=\"text\"\r\n                            name=\"address\"\r\n                            v-model=\"address\"\r\n                            class=\"form-control border-input\"\r\n                            placeholder=\"No Address set\"\r\n                          />\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n\r\n                    <div class=\"text-center\">\r\n                      <!--<button type=\"submit\" class=\"btn btn-info btn-fill btn-wd p-2\">Update Details</button>-->\r\n                      <button\r\n                        type=\"submit\"\r\n                        class=\"btn btn-success btn-fill btn-wd p-2 m-2\"\r\n                      >\r\n                        Update\r\n                      </button>\r\n                    </div>\r\n                    <div class=\"clearfix\"></div>\r\n                  </form>\r\n                  <!--End Of Store Login-->\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Sidebar from \"./Sidebar\";\r\nimport { fb } from \"../firebaseInit\";\r\nconst db = fb.firestore();\r\n\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"verifyDetails\",\r\n  components: {\r\n    Sidebar\r\n  },\r\n  data() {\r\n    return {\r\n      currentUser: fb.auth().currentUser,\r\n      verified: null,\r\n      storeId: sessionStorage.getItem(\"storeId\"),\r\n      errors: [],\r\n      email: null,\r\n      password: null,\r\n      address: null\r\n    };\r\n  },\r\n  created() {\r\n    this.loadData();\r\n  },\r\n  methods: {\r\n    processForm: function() {\r\n      axios\r\n        .post(\"http://localhost:3000/api/login\", {\r\n          email: this.email,\r\n          password: this.password,\r\n          address: this.address\r\n        })\r\n        .then(\r\n          response => {\r\n            console.log(response);\r\n            if (response.data.success == true) {\r\n              db.collection(\"users\")\r\n                .doc(this.currentUser.uid)\r\n                .set(\r\n                  {\r\n                    storeId: response.data.id\r\n                  },\r\n                  { merge: true }\r\n                );\r\n            }\r\n          },\r\n          error => {\r\n            console.log(error);\r\n          }\r\n        );\r\n    },\r\n    updateAddress: function() {\r\n      axios\r\n        .put(\"http://localhost:3000/api/customer/\" + this.storeId, {\r\n          address: this.address\r\n        })\r\n        .then(\r\n          response => {\r\n            // console.log(response);\r\n          },\r\n          error => {\r\n            console.log(error);\r\n          }\r\n        );\r\n    },\r\n    loadData: function() {\r\n      // you can load data from here and assign response in to variable\r\n      db.collection(\"users\")\r\n        .doc(this.currentUser.uid)\r\n        .get()\r\n        .then(function(doc) {\r\n          if (doc.exists && doc.get(\"storeId\")) {\r\n            console.log(\"doc exists\");\r\n            sessionStorage.setItem(\"storeId\", doc.get(\"storeId\"));\r\n          }\r\n        })\r\n        .catch(function(error) {\r\n          console.log(\"Error getting document:\", error);\r\n        });\r\n      if (sessionStorage.getItem(\"storeId\") != null) {\r\n        this.verified = true;\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style scoped>\r\n#verify-details {\r\n  height: 100%;\r\n  min-height: 100%;\r\n  /*background-color: #edf0f5; */\r\n}\r\nbody {\r\n  margin-top: 20px;\r\n  /* background: #fafafa; */\r\n}\r\n</style>\r\n"]}]}